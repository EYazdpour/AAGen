        "!pip install torchattacks\n",
        "import torchattacks\n",
        "\n",
        "def BIM (image, label, model):\n",
        "  atk = torchattacks.BIM(model, eps=8/255, alpha=2/255, steps=4)\n",
        "  adv_image = atk(image, label)\n",
        "  return adv_image"
